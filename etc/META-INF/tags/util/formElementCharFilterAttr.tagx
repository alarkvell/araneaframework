<jsp:root xmlns:jsp="http://java.sun.com/JSP/Page"
	xmlns:c="http://java.sun.com/jsp/jstl/core"
	xmlns:ui="http://araneaframework.org/tag-library/2.0/ui"
	version="2.1">

	<jsp:directive.tag description="Writes an attribute that defines character filter for an input." body-content="empty"/>

	<c:if test="${not empty control.inputFilter}">
		<ui:attribute name="arn-charFilter" value="${control.inputFilter.characterFilter}"/>
	</c:if>

</jsp:root>
