<ivysettings>
  <properties file="${ivy.conf.dir}/version.properties" />
  <properties file="${ivy.conf.dir}/ivyrep.properties" />
  <property name="ibiblio-maven2-pattern" value="[organisation]/[module]/[revision]/[artifact]-[revision].[type]" />
  <property name="ibiblio-maven2-root" value="http://www.ibiblio.org/maven2/" />
  <property name="ibiblio-araneaframework-pattern" value="[organisation]/[module]/[revision]/[artifact]-[revision].[type]" />
  <property name="ibiblio-araneaframework-root" value="http://www.araneaframework.org/repo/" />
  <settings defaultResolver="defaultChainResolver" />
  <resolvers>
    <chain name="defaultChainResolver" returnFirst="true">
      <url name="wm">
        <ivy pattern="http://tiiger.webmedia.int/ivy/[organisation]/[module]/ivys/ivy-[revision].xml" />
        <artifact pattern="http://tiiger.webmedia.int/ivy/[organisation]/[module]/[type]s/[artifact]-[revision].[type]" />
      </url>
      <ibiblio name="ibiblio" root="${ibiblio-maven2-root}" pattern="${ibiblio-maven2-pattern}" m2compatible="true"
        namespace="ibiblio" />
      <ibiblio name="ibiblio-araneaframework" root="${ibiblio-araneaframework-root}" pattern="${ibiblio-araneaframework-pattern}"
        m2compatible="false" />
    </chain>
    <filesystem name="repository" checkmodified="true">
      <ivy pattern="${aranea.ivy.repository}/[organisation]/[module]/[revision]/ivy-[revision].xml" />
      <artifact pattern="${aranea.ivy.repository}/[organisation]/[module]/[revision]/[artifact]-[revision].[ext]" />
    </filesystem>
  </resolvers>
  <namespaces>
    <namespace name="ibiblio">
      <rule>
        <fromsystem>
          <src org="apache" module="commons-beanutils" />
          <dest org="commons-beanutils" module="commons-beanutils-core" />
        </fromsystem>
        <tosystem>
          <src org="commons-beanutils" module="commons-beanutils-core" />
          <dest org="apache" module="commons-beanutils" />
        </tosystem>
      </rule>
      <rule>
        <fromsystem>
          <src org="apache" module=".+" />
          <src org="jakarta-regexp" module=".+" />
          <src org="log4j" module="log4j" />
          <src org="oro" module="oro" />
          <dest org="$m0" module="$m0" />
        </fromsystem>
        <tosystem>
          <src org="commons-.+" module="commons-.+" />
          <src org="jakarta-regexp" module="jakarta-regexp" />
          <src org="log4j" module="log4j" />
          <src org="oro" module="oro" />
          <dest org="apache" module="$m0" />
        </tosystem>
      </rule>
      <rule>
        <fromsystem>
          <src org="apache" module="taglibs-standard" />
          <dest org="taglibs" module="standard" />
        </fromsystem>
        <tosystem>
          <src org="taglibs" module="standard" />
          <dest org="apache" module="taglibs-standard" />
        </tosystem>
      </rule>
    </namespace>
  </namespaces>
</ivysettings>