<ivysettings>
	<properties file="${ivy.conf.dir}/version.properties" />

	<property name="ibiblio-aranea-pattern" value="[organisation]/[module]/[revision]/[artifact]-[revision].[type]" />
	<property name="ibiblio-aranea-root" value="http://www.araneaframework.org/repo/" />

	<settings defaultResolver="defaultChainResolver" />

	<resolvers>
		<chain name="defaultChainResolver">
			<ibiblio name="ibiblio-araneaframework" m2compatible="true" root="${ibiblio-aranea-root}" pattern="${ibiblio-aranea-pattern}"/>
			<ibiblio name="ibiblio-maven2" m2compatible="true" />
		</chain>
	</resolvers>

</ivysettings>
